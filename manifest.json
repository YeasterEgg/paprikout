{
  "name": "Paprikout",
  "version": "1.0",
  "description": "Just logs out of paprika once closed",
  "permissions": ["tabs"],
  "browser_action": {
    "default_title": "Paprikout"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
